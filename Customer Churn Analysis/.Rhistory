# Install and load required libraries
install.packages(c("tidyverse", "data.table"))  # Run only if not installed
library(tidyverse)
library(data.table)
# Load the dataset
file_path <- "churn data.csv"  # Update with the actual path
df <- fread(file_path)  # Fast data loading
# Display basic information about the dataset
glimpse(df)  # Overview of the dataset
summary(df)  # Summary statistics
head(df)  # First few rows
